<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/unite_top_tab_size"
        android:background="@drawable/home_tab_bg">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/setting_txt_top_title"
        android:layout_marginTop="@dimen/unite_txt_to_top_tab_size"
        android:textSize="@dimen/unite_top_tab_txt_size"
        android:textColor="@color/white"
        android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <Button
        android:layout_marginTop="20dp"
        android:id="@+id/btn_unLogin"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:text="@string/setting_txt_unlogin" />
</LinearLayout>
